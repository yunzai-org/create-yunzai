{
  "name": "yz-template",
  "version": "1.0.0-rc.0",
  "description": "Yunzai-Next APP",
  "author": "ningmengchongshui",
  "type": "module",
  "private": true,
  "scripts": {
    "app": "npx tsx src/main.ts",
    "img-dev": "npx tsx watch --clear-screen=false routes.server.ts",
    "build": "npm run css-m && rollup  --config rollup.config.js",
    "css-app": "tailwindcss -i ./src/input.css -o ./public/output.css",
    "css-dev": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "css-m": "tailwindcss -i ./src/input.css -o ./public/output.css -m",
    "format": "prettier --write ."
  },
  "dependencies": {
    "puppeteer": "^23.0.2",
    "react": "^18.3.1",
    "react-puppeteer": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "prettier": "^3.2.5",
    "rollup": "^4.16.4",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.18.0",
    "typescript": "^5.5.3",
    "yunzai": "^1.0.6",
    "yunzai-mys": "^1.0.5"
  },
  "types": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "keywords": [
    "yunzai"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
