{
  "name": "yz-template",
  "version": "1.0.0-rc.0",
  "description": "Yunzai-Next APP",
  "author": "ningmengchongshui",
  "type": "module",
  "private": true,
  "scripts": {
    "app": "npx tsx ts-node/esm src/main.js",
    "dev": "nodemon --config nodemon.json",
    "img-app": "npm run css && npx tsx routes.server.ts",
    "img-dev": "nodemon --config routes.nodemon.json",
    "build": "npm run css-m && rollup  --config rollup.config.js",
    "css-app": "tailwindcss -i ./src/input.css -o ./public/output.css",
    "css-dev": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "css-m": "tailwindcss -i ./src/input.css -o ./public/output.css -m",
    "format": "prettier --write ."
  },
  "dependencies": {
    "puppeteer": "^23.0.2",
    "react": "^18.3.1",
    "react-puppeteer": "^1.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "rollup": "^4.16.4",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3",
    "yunzai": "^1.0.6",
    "yunzai-mys": "^1.0.2"
  },
  "types": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "keywords": [
    "yunzai"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
