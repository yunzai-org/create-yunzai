{
  "name": "yz-template",
  "version": "1.0.0-rc.0",
  "description": "YunzaiJS插件",
  "author": "ningmengchongshui",
  "type": "module",
  "private": true,
  "scripts": {
    "app": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/main.ts",
    "dev": "nodemon",
    "css": "tailwindcss -i ./src/input.css -o ./public/output.css",
    "css:watch": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "format": "prettier --write ."
  },
  "dependencies": {
    "react-puppeteer": "^1.0.0-rc.7",
    "yunzai": "^1.0.0-rc.4"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "rollup": "^4.16.4",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3"
  },
  "files": [
    "index.js",
    "types"
  ],
  "types": "types",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/index.d.ts"
    }
  },
  "keywords": [
    "yunzai"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
